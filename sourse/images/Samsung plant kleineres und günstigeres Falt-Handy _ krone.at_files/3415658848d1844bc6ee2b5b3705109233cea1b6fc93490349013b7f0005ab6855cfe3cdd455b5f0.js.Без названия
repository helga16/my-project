/* CDS: 3415658848d1844bc6ee2b5b3705109233cea1b6fc93490349013b7f0005ab6855cfe3cdd455b5f0 at: 11.09.2019 14:51:55 */ function kmmTimeAgo(selectors){var $selectors=selectors||$(".js-timeago");var now=moment();$selectors.each(function(){if(!$(this).attr("data-datetime")){return true}var time=moment($(this).attr("data-datetime"),"X");if(now.isSame(time,"day")){if(now.diff(time,"minutes",true)<=60){$(this).text("vor "+now.diff(time,"minutes")+" Minute"+(now.diff(time,"minutes")>1?"n":""))}else{$(this).text(time.format("LT"))}}else{if(now.diff(time,"days",true)<=2){$(this).text("gestern, "+time.format("LT"))}else{$(this).text(time.format("L, LT"))}}})}$(document).ready(function(){kmmTimeAgo()});